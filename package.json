{
  "name": "@rnacentral/r2dt-web",
  "version": "1.0.1",
  "description": "Visualise RNA secondary structure using standard layouts.",
  "scripts": {
    "clean": "rm -rf dist/*",
    "build:prod": "node_modules/.bin/webpack --mode production",
    "build": "npm run clean && npm run build:prod",
    "profile:build": "node_modules/.bin/webpack --mode production --profile --json > webpack-stats.json",
    "serve": "node_modules/.bin/webpack-dev-server --mode development --progress --color --history-api-fallback",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RNAcentral/r2dt-web.git"
  },
  "keywords": [
    "RNA",
    "bioinformatics",
    "rna-secondary-structure"
  ],
  "contributors": [
    {
      "name": "Anton I. Petrov",
      "email": "apetrov@ebi.ac.uk"
    },
    {
      "name": "Carlos E. Ribas",
      "email": "cribas@ebi.ac.uk"
    }
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/RNAcentral/r2dt-web/issues"
  },
  "homepage": "https://github.com/RNAcentral/r2dt-web#readme",
  "dependencies": {
    "dotenv-webpack": "^7.1.0",
    "react": "^17.0.2",
    "react-bootstrap-typeahead": "^5.2.1",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-redux": "^7.2.6",
    "react-svg-pan-zoom": "^3.11.0",
    "react-svgmt": "^1.2.0",
