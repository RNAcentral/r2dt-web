    "babel-loader": "^8.2.3",
    "css-loader": "^6.6.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "navigator": "~1.0.1",
    "node-sass": "^7.0.1",
    "sass-loader": "^12.4.0",
